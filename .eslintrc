{
	"extends": ["airbnb", "airbnb/hooks", "prettier"],
	"plugins": ["prettier"],
	"parserOptions": {
	  "ecmaVersion": 2020,
	  "ecmaFeatures": {
		"jsx": true,
		"experimentalObjectRestSpread": true
	  }
	},
	"rules": {
		"linebreak-style": ["error", "windows"],
		"react/button-has-type": "off" ,
		"react/react-in-jsx-scope": "off",
		"no-undef": "off",
	}
  }
  